<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>DSR</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
        
<link rel="stylesheet" href="/Assests//css/dsr.css" >

    </head>

    <body class="container">


        <!-- Rest of your HTML content -->
        <% if (messages.success) { %>
        <div class="alert alert-success" role="alert"><%- messages.success %></div>
        <% } %>

        <% if (messages.error) { %>
        <div class="alert alert-danger" role="alert"><%- messages.error %></div>
        <% } %>

        <div class="card">
            <div class="card-header">
                <ul class="nav nav-pills w-100">
                    <li class="nav-pill active">
                        <a class="nav-link"><b>Manage DSR</b></a>
                    </li>

                    <li class="nav-pill">
                        <a class="nav-link active" href="/DSR/dsrBulk">
                            <i class="fas fa-upload"></i> DSR Upload
                        </a>
                    </li>
                    <li class="nav-pill">
                        <a class="nav-link active" href="/DSR/exportDSR">
                            <i class="fas fa-download"></i> Export All
                        </a>
                    </li>

                    <li class="nav-pill">
                        <a class="nav-link active" href="/master">
                            <i class="fas fa-home"></i> Data Management
                        </a>
                    </li>

                </ul>
            </div>
            <form action="/DSR/searchDSR" method="get" class="search-form">
                <input type="text" class="form-control" name="DSRCode" placeholder="DSR Code">
                <button type="submit" class="btn btn-primary">
                    <i class="fas fa-search"></i> Search
                </button>
                <a class="nav-link active" href="/DSR">
                    <i class="fas fa-undo"></i> Reset
                </a>
            </form>
            <div class="card-body">
                <% if(data.length) { %>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">DSRCode</th>
                            <th scope="col">DSRName</th>
                            <th scope="col">Mobile Number</th>
                            <th scope="col">Aadhar Number</th>
                            <th scope="col">Create Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i = 0; i< data.length; i++) { %>
                        <tr>
                            <th scope="row"><%= (i+1) %></th>
                            <td><%= data[i].DSRCode%></td>
                            <td><%= data[i].DSRName%></td>
                            <td><%= data[i].MobileNumber%></td>
                            <td><%= data[i].AadharNumber%></td>
                            <td><%= data[i].CreateDate%></td>

                        </tr>
                        <% } %>
                    </tbody>
                </table>
                <% } %>

                <!-- if result is empty -->
                <% if(!data.length) { %>
                <p class="text-center">No users found!</p>
                <% } %>
            </div>
        </div>


        <!-- Include jQuery and other necessary scripts -->
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>


    </body>

</html>